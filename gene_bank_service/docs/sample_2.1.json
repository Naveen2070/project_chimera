{
  "$schema": "https://schemas.sourcemeta.com/openapi/v3.1/schema/2022-10-07.json",
  "openapi": "3.1.0",
  "info": {
    "description": "This API provides endpoints for gene bank service which is a part of the project chimera.",
    "title": "Gene Bank Service API",
    "contact": {
      "name": "Naveen R",
      "url": "https://naveen2070.github.io/portfolio",
      "email": "naveenrameshcud@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0.0"
  },
  "paths": {
    "/actuator:/health": {
      "get": {
        "description": "Get actuator health status based on queue and RabbitMQ statuses.",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ActuatorSuccessResponse"
                }
              }
            }
          },
          "500": {
            "description": "Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ActuatorErrorResponse"
                }
              }
            }
          }
        },
        "tags": ["Actuator"],
        "summary": "Get actuator health status"
      }
    }
  },
  "components": {
    "schemas": {
      "ActuatorErrorResponse": {
        "type": "object",
        "properties": {
          "error": {
            "type": "string"
          }
        }
      },
      "ActuatorSuccessResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "ApiKeyAuth": []
    }
  ]
}
