generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum PostType {
  public
  private
}

model Flora {
  id              String   @id @default(uuid()) @db.Uuid
  common_name     String
  scientific_name String
  user_id         String
  type            PostType
}
