{
    "$schema": "https://steeltoe.io/schema/v3/schema.json",
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Consul": {
        "Discovery": {
            "HostName": "localhost",
            "Port": 7149,
            "Scheme": "https",
            "ServiceName": "user-service",
            "PreferIpAddress": true,
            "HealthCheckPath": "/actuator/health",
            "HealthCheckInterval": "10s"
        }
    },
    "AllowedHosts": "*"
}
